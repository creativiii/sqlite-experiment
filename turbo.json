{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "meow:tailwind": {
      "cache": false,
      "inputs": ["meow-cms"]
    },
    "docs:dev": {
      "cache": true,
      "inputs": ["apps/docs"]
    },
    "test:dev": {
      "cache": true,
      "inputs": ["apps/cms"]
    },
    "docs": {
      "dependsOn": ["meow:tailwind", "docs:dev"]
    },
    "test": {
      "dependsOn": ["meow:tailwind", "test:dev"]
    },
    "docs:build": {},
    "meow:release": {},
    "meow:build": {}
  }
}
