    "@mdx-js/react": "^3.0.1",
    "@octokit/types": "^7.5.0",
    "@prezly/slate-lists": "^0.102.0",
    "@tanstack/query-core": "^4.24.4",
    "@tanstack/react-query": "^4.24.4",
    "@tanstack/react-query-devtools": "^4.36.1",
    "axios": "^0.27.2",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^0.2.1",
    "dayjs": "^1.11.13",
    "escape-html": "^1.0.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.7",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "lodash": "^4.17.21",
    "mdx": "^0.3.1",
    "mime": "^3.0.0",
    "octokit": "^2.0.7",
    "phosphor-react": "^1.4.1",
    "prism": "^4.1.2",
    "prismjs": "^1.29.0",
    "react-error-boundary": "^3.1.4",
