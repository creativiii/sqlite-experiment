{
  "version": "0.6.26",
  "name": "imput-cms",
  "main": "dist/index.js",
  "module": "dist/imput.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "imput:build": "tsc -v && tsc && pnpm imput:tailwind && tsup src/index.tsx --format cjs,esm --dts --minify",
    "imput:release": "release publish",
    "imput:tailwind": "pnpm dlx tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css --minify",
    "imput:tailwind:watch": "pnpm dlx tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css --watch=always --minify",
    "imput:test": "vitest run",
    "imput:test:dev": "vitest --watch --t \"serialization\"",
    "lint": "dts lint",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cms",
    "markdown",
    "mdx"
  ],
  "description": "An MDX-powered CMS.",
  "dependencies": {
    "@jongwooo/prism-theme-github": "^1.7.0",
    "@juggle/resize-observer": "^3.4.0",
    "@lukemorales/query-key-factory": "^1.1.0",
